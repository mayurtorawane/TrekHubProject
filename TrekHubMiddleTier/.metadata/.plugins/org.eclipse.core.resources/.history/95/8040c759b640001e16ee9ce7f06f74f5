package com.example.demo.entities;

import java.sql.Date;

public class TrekBookingDummy {

	
	Date booking_date;
	int trekker_id;
	int plantrek_id;
	int members;
	double amount;
	public TrekBookingDummy(Date booking_date, int trekker_id, int plantrek_id, int members, double amount) {
		super();
		this.booking_date = booking_date;
		this.trekker_id = trekker_id;
		this.plantrek_id = plantrek_id;
		this.members = members;
		this.amount = amount;
	}
	public TrekBookingDummy() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Date getBooking_date() {
		return booking_date;
	}
	public void setBooking_date(Date booking_date) {
		this.booking_date = booking_date;
	}
	public int getTrekker_id() {
		return trekker_id;
	}
	public void setTrekker_id(int trekker_id) {
		this.trekker_id = trekker_id;
	}
	public int getPlantrek_id() {
		return plantrek_id;
	}
	public void setPlantrek_id(int plantrek_id) {
		this.plantrek_id = plantrek_id;
	}
	public int getMembers() {
		return members;
	}
	public void setMembers(int members) {
		this.members = members;
	}
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	
	
}
